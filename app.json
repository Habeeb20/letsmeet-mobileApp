{
  "expo": {
    "name": "letsmeet",
    "slug": "letsmeet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/myLogo.png",
    "scheme": "letsmeet",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/myLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/myLogo.png",
        "backgroundColor": "#ffffff",
        "package": "com.ademola.letsmeet"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ademolawaliyu.letsmeet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b06c0ff6-104c-4a11-8b9e-519bfe051def"
      }
    }
  }
}
